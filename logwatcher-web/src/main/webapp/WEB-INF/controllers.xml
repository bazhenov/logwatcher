<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
			 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			 xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.farpost.logwatcher.web.FeedController">
		<property name="storage" ref="storage"/>
	</bean>

	<bean class="com.farpost.logwatcher.web.BackController">
		<property name="storage" ref="storage"/>
	</bean>

	<bean class="com.farpost.logwatcher.web.LayoutController"/>

	<bean class="com.farpost.logwatcher.web.SessionController"/>

	<bean class="com.farpost.logwatcher.web.DashboardController">
		<constructor-arg ref="storage"/>
	</bean>
</beans>